{
    "env": {
        "es2021": true
    },
    "parser": "@typescript-eslint/parser",
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "semi": [
            "error",
            "always"
        ],
        "quotes": [
            "error",
            "double"
        ],
        "no-empty": [
            "error"
        ],
        "no-extra-semi": [
            "error"
        ],
        "curly": [
            "error"
        ],
        "eqeqeq": [
            "error"
        ],
        "no-implied-eval": [
            "error"
        ]
    }
}